datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL") // Set your DATABASE_URL in the .env file
}

generator client {
    provider = "prisma-client-js"
}

model Student {
    id       Int     @id @default(autoincrement())
    name     String
    email    String  @unique
    classId  Int
}

model Teacher {
    id       Int     @id @default(autoincrement())
    name     String
    email    String  @unique
}

model Class {
    id       Int     @id @default(autoincrement())
    name     String
}

// Define other models as per your requirements
